type Comment {
  id: ID! @id @unique
  comment: String!
  author: String!
  dislikes: Int!
  date: String!
  message: Message
}

type Message {
  id: ID! @id @unique
  title: String!
  message: String!
  author: String!
  dislikes: Int!
  comments: [Comment]
  date: String
}

type User {
  id: ID! @id
  email: String! @unique
  userName: String! @unique
  firstName: String!
  lastName: String!
  password: String!
  messages: [Message]
}